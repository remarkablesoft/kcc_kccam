        "animated-scroll-to": "^2.0.12",
        "cors": "^2.8.5",
        "crypto-js": "^4.0.0",
        "dropzone-nuxt": "^0.1.3",
        "highcharts": "^7.0.1",
        "highcharts-vue": "^1.3.5",
        "jquery": "^3.5.1",
        "js-cookie": "^2.2.1",
        "liquor-tree": "^0.2.70",
        "lodash": "^4.17.20",
        "nuxt": "^2.14.6",
        "nuxt-element-ui": "^1.0.10",
