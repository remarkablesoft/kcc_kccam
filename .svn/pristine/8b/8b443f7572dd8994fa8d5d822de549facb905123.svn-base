@Setter
@Accessors( chain = true )
@ToString
public class ExcelJsonCnd implements Serializable {

		/**
		 *
		 */
		private static final long serialVersionUID = 6935544991401987004L;


		private String fileName;							// 파일명
		private String sheetName;							// 엑셀의 시트명
		private String displayHeader;						// 컬럼명 ex) No.,OID,SYSTEM_OID,NAME,CLASS_NAME,INPUT_USER,INPUT_DATE
		private String jsonData;							// 컬럼에 나올 데이터 json형태



}
